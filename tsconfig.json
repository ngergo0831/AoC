{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "noImplicitAny": false
    },
    "include": ["tasks/**/*.ts"],
    "exclude": ["node_modules"],
    "assets":["tasks/**/*.txt"]
}
